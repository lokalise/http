{
    "name": "logics/http",
    "keywords": ["http", "client", "form", "scraping"],
    "description": "HTTP client and form scraper",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir Bashkirtsev",
            "email": "vladimir@bashkirtsev.com",
            "homepage": "http://www.bashkirtsev.com",
            "role": "Developer"
